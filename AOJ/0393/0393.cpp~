#include<cassert>
using namespace std;
static const int H_MIN = 1;
static const int H_MAX = 100000;
int main() {
  int H, A, B; cin >> H >> A >> B;

  assert(H_MIN <= H && H <= H_MAX);
    assert(1 <= A && A <= B && B <= H );

  int cnt = 0;

  for ( int i = A; i <= B; i++ ){
      cnt += (H % i == 0 );
        }

  cout << cnt << endl;

}
